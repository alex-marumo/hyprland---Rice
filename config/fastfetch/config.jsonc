      "format": "{user-name}",
    },
    {
      "key": "  {#36}󰇅 HOST    ",
      "type": "title",
      "format": "{host-name}",
    },
    {
      "key": "  {#35}󰅐 UPTIME  ",
      "type": "uptime",
    },
    {
      "key": "  {#34}󰇄 DESKTOP ",
      "type": "de",
    },
    {
      "key": "  {#36} TERM    ",
      "type": "terminal",
    },
    "break",
    {
      "type": "custom",
      "key": "  ",
      "format": "\u001b[31m▰▰▰▰\u001b[36m▰▰▰▰\u001b[35m▰▰▰▰\u001b[37m▰▰▰▰",
    },
    "break",
    {
      "type": "custom",
      "key": "  ",
      "format": "\u001b[31m[ GOOD FOR HEALTH // BAD FOR EDUCATION ]",
    },
  ],
}
