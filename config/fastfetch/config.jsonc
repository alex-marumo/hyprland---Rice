{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty",
    "source": "~/.config/fastfetch/images/akira.png",
    "color": {
      "1": "red", // Akira Red
      "2": "cyan", // Neon Cyan
    },
    "padding": {
      "top": 2,
      "left": 4
    },
  },
  "display": {
    "separator": " ⟫ ",
  },
  "modules": [
